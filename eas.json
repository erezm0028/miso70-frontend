{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "local": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "standalone": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "http://10.100.102.9:3001"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
